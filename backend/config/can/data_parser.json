                    "05": "HV ON",
                    "06": "Error",
                    "07": "TubeSwitching",
                    "08": "Armed",
                    "09": "Diag"
                }
            },
            "1": {
                "name": "ErrCode",
                "type": "hex8",
                "hideIfZero": true
            },
            "2": {
                "name": "Display",
                "type": "hex8",
                "hideIfZero": true
            },
            "3": {
                "name": "ErrClass",
                "type": "hex8",
                "hideIfZero": true
            },
            "4-5": {
