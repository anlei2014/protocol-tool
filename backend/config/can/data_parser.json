{
    "1cc": {
        "name": "Command Bitmap",
        "bytes": {
            "0": {
                "name": "Cmd",
                "type": "enum",
                "values": {
                    "00": "Stop Rotor/Filament",
                    "01": "Hold Rotor",
                    "03": "Prep Rotor/Filament",
                    "04": "Diag Mode",
                    "07": "Start Exposure",
                    "ff": "Idle (0 Amps)"
                }
            }
        }
    },
    "208": {
        "name": "Generator Status",
        "bytes": {
            "0": {
                "name": "Phase",
                "type": "enum",
                "values": {
                    "00": "Idle/Init",
                    "01": "PoweredUp/Idle",
                    "02": "StandBy",
                    "03": "PrepInProgress",
